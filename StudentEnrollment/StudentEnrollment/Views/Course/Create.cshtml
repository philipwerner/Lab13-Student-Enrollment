@model StudentEnrollment.Models.Course
@{ }
<h2>Create a New Course</h2>
<form asp-action="Create">
    <label asp-for="Name"></label>
    <input asp-for="Name" />
    <span asp-validation-for="Name"></span>
    
    <label asp-for="Level"></label>
    <input asp-for="Level" />
    <span asp-validation-for="Level"></span>

    <label asp-for="Instructor"></label>
    <input asp-for="Instructor" />
    <span asp-validation-for="Instructor"></span>

    <input type="submit" value="Create Course"/>
</form>

<a asp-action="Index">Return to Courses</a>
